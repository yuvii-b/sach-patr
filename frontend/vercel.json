{
  "version": 2,
  "buildCommand": "npm run build:prod",
  "installCommand": "npm install",
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "public" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" }
  ]
}
